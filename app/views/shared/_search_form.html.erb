<%= search_form_for [:admin, @q] do |f| %>
  <%= f.label :status_eq %>
  <%= f.select("object", options_for_select([[t(".ordered"), @selects[0]],
    [t(".taking"), @selects[1]], [t(".give_back"), @selects[2]]], params[:status_eq])) %>

  <%= f.label :created_at_cont %>
  <%= f.date_field :created_at_date_equals,
    autocomplete: "off", onkeydown: "return false" %>
  <%= f.submit %>
<% end %>
