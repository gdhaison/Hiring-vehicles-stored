<tr>
  <td><%= vehicle.category_name %></td>
  <td><%= vehicle.name %></td>
  <td><%= image_tag check_image(vehicle), id: "admin_id"%></td>
  <td><%= vehicle.branch_name %></td>
  <td><%= vehicle.price %>$/H</td>
  <td><%= vehicle.quantity %></td>
  <td><%= vehicle.description %></td>
  <td><%= link_to t(".edit"), edit_admin_vehicle_path(id: vehicle.id),
    class: "btn btn-primary", remote: true %></td>
  <td><%= link_to t(".delete"),admin_vehicle_path(vehicle), data: { confirm: t(".sure?")},
    method: :delete, remote: true, class: "btn btn-danger destroy-vehicle" %></td>
</tr>
